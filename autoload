# File to be imported from .bashrc that autoloads all of the commands the repo defines.

# Loads wsl specific commands.
if [ -f ~/bash-linux/wsl ]; then
    . ~/bash-linux/wsl
fi

# Loads git helpers commands
if [ -f ~/bash-linux/githelpers ]; then
    . ~/bash-linux/githelpers
fi

# Loads utilities commands
if [ -f ~/bash-linux/utilities ]; then
    . ~/bash-linux/utilities
fi